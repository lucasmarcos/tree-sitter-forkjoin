{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "forkjoin",
  "rules": {
    "source_file": {
      "type": "REPEAT",
      "content": {
        "type": "SYMBOL",
        "name": "_sttm"
      }
    },
    "_sttm": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_expr"
        },
        {
          "type": "SYMBOL",
          "name": "def"
        }
      ]
    },
    "_expr": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "fork"
        },
        {
          "type": "SYMBOL",
          "name": "join"
        },
        {
          "type": "SYMBOL",
          "name": "assign"
        },
        {
          "type": "SYMBOL",
          "name": "call"
        }
      ]
    },
    "fork": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "FORK"
        },
        {
          "type": "SYMBOL",
          "name": "label"
        },
        {
          "type": "SYMBOL",
          "name": "semi"
        }
      ]
    },
    "join": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "JOIN"
        },
        {
          "type": "SYMBOL",
          "name": "label"
        },
        {
          "type": "STRING",
          "value": ","
        },
        {
          "type": "SYMBOL",
          "name": "label"
        },
        {
          "type": "STRING",
          "value": ","
        },
        {
          "type": "SYMBOL",
          "name": "label"
        },
        {
          "type": "SYMBOL",
          "name": "semi"
        }
      ]
    },
    "assign": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "label"
        },
        {
          "type": "STRING",
          "value": "="
        },
        {
          "type": "SYMBOL",
          "name": "digit"
        },
        {
          "type": "SYMBOL",
          "name": "semi"
        }
      ]
    },
    "call": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "label"
        },
        {
          "type": "SYMBOL",
          "name": "semi"
        }
      ]
    },
    "def": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "label"
        },
        {
          "type": "STRING",
          "value": ":"
        }
      ]
    },
    "label": {
      "type": "PATTERN",
      "value": "[a-zA-Z_\\\\'][a-zA-Z_\\\\'0-9]*"
    },
    "digit": {
      "type": "PATTERN",
      "value": "[0-9][0-9]*"
    },
    "comment": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "//"
        },
        {
          "type": "PATTERN",
          "value": ".*"
        }
      ]
    },
    "semi": {
      "type": "STRING",
      "value": ";"
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    },
    {
      "type": "SYMBOL",
      "name": "comment"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": []
}
